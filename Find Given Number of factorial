import java.util.*;

public class Main
{
	public static void main(String[] args) {
		System.out.println("Find Given Number of factorial");
		int n,fact=1;
		Scanner sc= new Scanner(System.in);
		System.out.println("Enter Number here");
		n=sc.nextInt();
		while(n>0){
		    fact = fact*n;
		    n=n-1;
		}
		System.out.println("Factorial is :"+fact);
	}
}
