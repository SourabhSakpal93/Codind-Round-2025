import java.util.*;
public class Main
{
  public static void main (String[]args)
  {
	System.out.println ("This Program For a find Reverse for number");
	int n, rev = 0;
	Scanner sc = new Scanner (System.in);
	  System.out.println ("Enter number Here");
	  n = sc.nextInt ();
	while (n > 0)
	  {
		rev = (rev * 10) + n % 10;
		n = n / 10;
	  }
	System.out.println ("Reverve of Given Number :" + rev);
  }
}
